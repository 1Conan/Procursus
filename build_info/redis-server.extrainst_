#!/bin/sh

if [[ $1 == upgrade ]]; then
    /usr/bin/launchctl unload /Library/LaunchDaemons/io.redis.redis-server.plist
    /usr/bin/launchctl load /Library/LaunchDaemons/io.redis.redis-server.plist
fi

if [[ $1 == install ]]; then
    /usr/bin/launchctl load -w /Library/LaunchDaemons/io.redis.redis-server.plist
fi

exit 0
